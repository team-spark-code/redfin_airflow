  <!-- Google tag (gtag.js) -->
  <script type="text/javascript" src="/static/js/jquery.cookie.js"></script>
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-ECXMW3R212"></script>
  
  <script>
  
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}

    var check_site_id = 219;

    gtag('js', new Date());

    if(check_site_id === 354 || check_site_id === 68)
    {
      gtag('config', 'G-ECXMW3R212', { 'restricted_data_processing': true });
    }
    else
    {
      gtag('config', 'G-ECXMW3R212');
    }

    var measurement_id = 'G-ECXMW3R212';

    var gcid = '';

    const sessionIdPromise =  new  Promise(resolve => {
        gtag('get', 'G-ECXMW3R212', 'session_id', resolve)
    });
    const sessionNumPromise =  new  Promise(resolve => {
        gtag('get', 'G-ECXMW3R212', 'session_number', resolve)
    });
    const clientID =  new  Promise(resolve => {
        gtag('get', 'G-ECXMW3R212', 'client_id', resolve)
    });

    var ga_four_event = function(type,title,id){
        try {
                
                    gtag('event',type, {
                                'event_category':type,
                                'event_action' :title, 
                                'event_label' :id,
                                'event_type' : 'frontend',
                                'client_identity':gcid,
                                'send_to': measurement_id 
                          });
                
        }
        catch(e) {

        }
    }

    function ga_event(type, title, id) {
      console.warn("DEPRECATED: The function 'ga_event' was called but is no longer supported.", { type: type, title: title, id: id });
    }

    function ga_social_event(network, action, target) {
      console.warn("DEPRECATED: The function 'ga_social_event' was called but is no longer supported.", { network: network, action: action, target: target });
    }

    Promise.all([sessionIdPromise, sessionNumPromise, clientID]).then(function(session_data) {

            gcid = session_data[2]
            
            

            gtag('set', 'user_properties', {'client_identity':session_data[2]});

            
                  var c_group1 = 'empty';
            

            if(jQuery(".ipr_ga_mixed_gallery_page_flag").length > 0)
            {
                 var c_group2 = 'mixed_gallery';
            }
            else
            {
                 var c_group2 = 'empty';
            }

            
                 var c_group4 = '5432fabac80467062000a6b0';
            


            
                    var c_group3 = 'empty';
            


            gtag('event', 'content_group', { //general tracking for content groups
                     'content_group1':c_group1,
                     'content_group2':c_group2,
                     'content_group3':c_group3,
                     'content_group4':c_group4,
                     'event_type' : 'frontend',
                     'client_identity':gcid,
                     
                     'send_to': measurement_id
            });

            

            

            

            
                      ga_four_event('PublishDate','View','December 7, 2011');
            

            

            

            

            

            var ga_referrer = document.referrer;
                if(ga_referrer != "" && ga_referrer.indexOf('?q') >=0){
                  var qs = ga_referrer.split('?q=')[1] || '';
                  var final_qs = qs.split('&')[0]
                  
                    ga_four_event('Search',final_qs,'/rss');
                    //alert("send");
                  
            }

    });

    var social_patterns = ['facebook.com/share','twitter.com/share','linkedin.com/share','pinterest.com/share','mailto:?subject'];

    jQuery( document ).ready(function(){

        try {
            jQuery(".mixed_gallery_low_res_download, .mixed_gallery_hi_res_download").click(function(){
                   ga_four_event('mixed_gallery_download',jQuery(this).attr('title'),jQuery(this).attr('data-ipr-ga'));
            });
        }
        catch(e){

        }

        try {
            jQuery(".ga-fileview").click(function(){
              page_id = "5432fabac80467062000a6b0";
              user_id = "";
              if(user_id == ""){user_id='ANONYMOUS'}
              track_event_label = page_id+"_"+user_id;
                 ga_four_event('fileView',jQuery(this).attr('data-ipr-ga'),track_event_label);
            });
        }
        catch(e){

        }

        try {
            jQuery('a').click(function(){
                if(jQuery(this).attr("href") != '' || jQuery(this).attr("href").startsWith("#")){

                    var href = jQuery(this).attr("href")

                    var ismatched = false

                    var social_action = "notset"

                    ga_four_event('linkClick',jQuery(this).attr("href"),jQuery(this).text());
                    
                    for (const pattern of social_patterns) {
                        if(href.includes(pattern)) {
                          ismatched = true
                          if(pattern.includes("facebook")){
                            social_action = "facebook";
                          }
                          else if(pattern.includes("twitter")){
                            social_action = "twitter";
                          }
                          else if(pattern.includes("linkedin")){
                            social_action = "linkedin";
                          }
                          else if(pattern.includes("pinterest")){
                            social_action = "pinterest";
                          }
                          else if(pattern.includes("mailto")){
                            social_action = "email";
                          }
                          break;
                        }
                    }

                    if(ismatched){
                        ga_four_event('social',social_action,'https://nvidianews.nvidia.com/rss');
                    }

                }

            });
        }
        catch(e){

        }
    });


    var audio_view = function(file_id){
          page_id = "5432fabac80467062000a6b0";
          user_id = "";
          if(user_id == ""){user_id='ANONYMOUS'}
          track_event_label = page_id+"_"+user_id;
          ga_four_event('fileView',file_id,track_event_label);
          }

    var file_view = function(file_id){
          page_id = "5432fabac80467062000a6b0";
          user_id = "";
          if(user_id == "")
          {
            
               user_id='ANONYMOUS'
            
          }
          track_event_label = page_id+"_"+user_id;
          ga_four_event('fileView',file_id,track_event_label);
          
          }

    var video_view = function(file_id){
          page_id = "5432fabac80467062000a6b0";
          user_id = "";
          if(user_id == ""){user_id='ANONYMOUS'}
          track_event_label = page_id+"_"+user_id;
          ga_four_event('fileView',file_id,track_event_label);
          }

    $(window).on("load", function() {

          if($('video').length){
             $('video').each(function(){
                if( $(this).attr('data-fileid') != undefined || $(this).attr('data-fileid') != '')
                { 
                 file_view($(this).attr('data-fileid'))
                }
                else if($(this).closest('.cb-video'))
                { 
                 if($(this).closest('.cb-video').dataset.filesourceid != undefined)
                 {file_view($(this).closest('.cb-video').dataset.filesourceid)}    
                }
             });
          }

          if($('iframe').length){
            $('iframe').each(function(){
                if($(this).contents().find('video').length){
                    if( $(this).contents().find('video').attr('data-fileid') != undefined || $(this).contents().find('video').attr('data-fileid') != '')
                    { 
                     file_view($(this).contents().find('video').attr('data-fileid'))
                    }
                }
            });
          }

    });

  </script>




  
  

  


  
  

  
     
